{
  "name": "react-flickr-gallery",
  "version": "1.0.0",
  "description": "Flickr photo browser using react and flux",
  "author": "Stephen White",
  "main": "index.js",
  "scripts": {
    "test": "BABEL_JEST_STAGE=0 jest",
    "start": "env UV_THREADPOOL_SIZE=100 webpack-dev-server --inline --content-base build",
    "build": "NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack -p --config webpack.production.config.js",
    "postinstall": "NODE_ENV=production node deploy"
  },
  "license": "ISC",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "babel-runtime": "^5.6.15",
    "core-js": "^0.9.18",
    "css-loader": "^0.10.1",
    "events": "^1.0.2",
    "expose-loader": "^0.6.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.1",
    "flux": "^2.0.1",
    "immstruct": "^1.4.1",
    "immutable": "^3.7.3",
    "jquery": "^2.1.3",
    "mkdirp": "^0.5.0",
    "node-sass": "3.2.0",
    "object-assign": "^2.1.1",
    "react": "^0.13.3",
    "react-tween-state": "0.0.5",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "jest": "^0.1.40",
    "jest-cli": "^0.4.18"
  }
}
